{
  "seleniumDriver": {
    "type": "GoogleChrome"
  },
  "pipelines": [
    {
      "testing": false,
      "cron": "0 * * * *",
      "jitterSeconds": 300,
      "tracker": {
        "type": "File",
        "configuration": {
          "file": "seen.txt"
        }
      },
      "source": {
        "type": "Facebook",
        "configuration": {
          "groupAddress": "https://www.facebook.com/groups/dogspotting/"
        }
      },
      "extractor": {
        "type": "FacebookHtml"
      }
    },
    {
      "testing": false,
      "cron": "0 * * * *",
      "jitterSeconds": 300,
      "tracker": {
        "type": "File",
        "configuration": {
          "file": "seen.txt"
        }
      },
      "source": {
        "type": "Twitter",
        "configuration": {
          "profileAddress": "https://twitter.com/microsoft"
        }
      },
      "extractor": {
        "type": "TwitterHtml"
      }
    }
  ]
}